<div id="<%= dom_id batch %>">
  <p>
    <strong>Batch Id:</strong>
    <%= batch.id %>
  </p>

  <p>
    <strong>Name:</strong>
    <%= batch.name %>
  </p>

  <% if batch.students.present? %>
    <strong>Stundent Info:</strong>
    <% batch.students.each do |student| %>
      <p>
        <span><%= student.name %> | <%= student.email%> | <%= student.phone %><span>
      </p>
    <% end %>
  <% end %>
</div>
